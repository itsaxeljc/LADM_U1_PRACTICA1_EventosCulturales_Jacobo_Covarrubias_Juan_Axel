<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecondFragment">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="253dp"
        android:layout_height="24dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="14dp"
        android:fontFamily="sans-serif-medium"
        android:text="Seleccione una fecha:"
        android:textSize="16sp"
        android:typeface="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cover2"
        app:layout_constraintVertical_bias="0.333" />

    <ImageView
        android:id="@+id/cover2"
        android:layout_width="182dp"
        android:layout_height="106dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/cover"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_first2" />

    <TextView
        android:id="@+id/city"
        android:layout_width="140dp"
        android:layout_height="19dp"
        android:layout_marginStart="232dp"
        android:layout_marginTop="7dp"
        android:fontFamily="sans-serif-medium"
        android:text="Guadalajara, Jalisco"
        android:textColor="#3F51B5"
        android:textSize="12sp"
        android:typeface="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.333" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="138dp"
        android:layout_height="57dp"
        android:layout_marginStart="228dp"
        android:layout_marginTop="96dp"
        android:fontFamily="sans-serif-black"
        android:text="Lights by Belin d'Artegui"
        android:textSize="20sp"
        android:typeface="normal"
        app:layout_constraintBottom_toTopOf="@+id/city"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.333" />

    <TextView
        android:id="@+id/textview_first2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:foregroundTint="@color/negro49"
        android:paddingTop="10dp"
        android:text="Comprar Boletos"
        android:textAlignment="viewStart"
        android:textColor="@color/negro49"
        android:textSize="24sp"
        android:textStyle="bold"
        android:typeface="sans"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.192"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.055" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="140dp"
        android:layout_height="19dp"
        android:layout_marginStart="232dp"
        android:layout_marginTop="1dp"
        android:fontFamily="sans-serif-medium"
        android:text="Fechas disponibles"
        android:textColor="#9C27B0"
        android:textSize="10sp"
        android:typeface="normal"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/city"
        app:layout_constraintVertical_bias="0.333" />

    <Button
        android:id="@+id/cancelar"
        android:layout_width="145dp"
        android:layout_height="44dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/roundedbutton"
        android:backgroundTint="@color/white"
        android:text="Cancelar"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.131"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cantidad"
        app:layout_constraintVertical_bias="0.135" />

    <Button
        android:id="@+id/confirmar"
        android:layout_width="145dp"
        android:layout_height="44dp"
        android:background="@drawable/roundedbutton"
        android:backgroundTint="@color/negro49"
        android:text="Comprar"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/cancelar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.845"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/cancelar"
        app:layout_constraintVertical_bias="0.383" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="68dp"
        android:layout_height="23dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="16dp"
        android:fontFamily="sans-serif-light"
        android:text="Cantidad"
        android:textSize="16sp"
        android:typeface="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/calendarView" />

    <EditText
        android:id="@+id/cantidad"
        android:layout_width="67dp"
        android:layout_height="50dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="4dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/total"
        android:layout_width="181dp"
        android:layout_height="31dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/vip"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="55dp"
        android:layout_height="23dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="16dp"
        android:fontFamily="sans-serif-light"
        android:text="Total:"
        android:textAlignment="viewStart"
        android:textSize="16sp"
        android:typeface="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/calendarView" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="34dp"
        android:layout_height="22dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="16dp"
        android:fontFamily="sans-serif-light"
        android:text="VIP"
        android:textAlignment="center"
        android:textSize="16sp"
        android:typeface="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/calendarView" />

    <CalendarView
        android:id="@+id/calendarView"
        android:layout_width="334dp"
        android:layout_height="302dp"
        android:selectedWeekBackgroundColor="@color/negro49"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Switch
        android:id="@+id/vip"
        android:layout_width="55dp"
        android:layout_height="35dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toEndOf="@+id/cantidad"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

</androidx.constraintlayout.widget.ConstraintLayout>